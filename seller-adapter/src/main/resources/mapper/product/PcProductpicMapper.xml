	<resultMap id="BaseResultMap" type="com.hjy.entity.product.PcProductpic">
		<id column="zid" jdbcType="INTEGER" property="zid" />
		<result column="uid" jdbcType="CHAR" property="uid" />
		<result column="product_code" jdbcType="VARCHAR" property="productCode" />
		<result column="sku_code" jdbcType="VARCHAR" property="skuCode" />
		<result column="pic_url" jdbcType="VARCHAR" property="picUrl" />
		<result column="product_code_old" jdbcType="VARCHAR" property="productCodeOld" />
	</resultMap>

	<sql id="Example_Where_Clause">
		<where>
			<foreach collection="oredCriteria" item="criteria" separator="or">
				<if test="criteria.valid">
					<trim prefix="(" prefixOverrides="and" suffix=")">
						<foreach collection="criteria.criteria" item="criterion">
							<choose>
								<when test="criterion.noValue">
									and ${criterion.condition}
								</when>
								<when test="criterion.singleValue">
									and ${criterion.condition} #{criterion.value}
								</when>
								<when test="criterion.betweenValue">
									and ${criterion.condition} #{criterion.value}
									and
									#{criterion.secondValue}
								</when>
								<when test="criterion.listValue">
									and ${criterion.condition}
									<foreach close=")" collection="criterion.value" item="listItem"
										open="(" separator=",">
										#{listItem}
									</foreach>
								</when>
							</choose>
						</foreach>
					</trim>
				</if>
			</foreach>
		</where>
	</sql>

	<insert id="insertSelective" parameterType="com.hjy.entity.product.PcProductpic">
		insert into pc_productpic
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="zid != null">
				zid,
			</if>
			<if test="uid != null">
				uid,
			</if>
			<if test="productCode != null">
				product_code,
			</if>
			<if test="skuCode != null">
				sku_code,
			</if>
			<if test="picUrl != null">
				pic_url,
			</if>
			<if test="productCodeOld != null">
				product_code_old,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="zid != null">
				#{zid,jdbcType=INTEGER},
			</if>
			<if test="uid != null">
				#{uid,jdbcType=CHAR},
			</if>
			<if test="productCode != null">
				#{productCode,jdbcType=VARCHAR},
			</if>
			<if test="skuCode != null">
				#{skuCode,jdbcType=VARCHAR},
			</if>
			<if test="picUrl != null">
				#{picUrl,jdbcType=VARCHAR},
			</if>
			<if test="productCodeOld != null">
				#{productCodeOld,jdbcType=VARCHAR},
			</if>
		</trim>
	</insert>

	<select id="findListBySkuNull" resultMap="BaseResultMap" parameterType="com.hjy.entity.product.PcProductpic">
		select
		* from productcenter.pc_productpic
		where 1=1
		<if test="productCode != null ">
			and product_code = #{productCode , jdbcType=VARCHAR}
		</if>
		and (sku_code = '' or sku_code is null)
	</select>

	<delete id="deleteByExample" parameterType="com.hjy.entity.product.PcProductpicExample">
		delete from pc_productpic
		<if test="_parameter != null">
			<include refid="Example_Where_Clause" />
		</if>
	</delete>
